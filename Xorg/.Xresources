Xft.dpi:        96
Xft.antialias:  true
Xft.hinting:    true
Xft.hintstyle:  hintslight
Xft.rgba:       rgb

! special
*.foreground:   #c5c9c5
*.background:   #181616
*.cursorColor:  #c5c9c5

! black
*.color0:       #0d0c0c
*.color8:       #a6a69c

! red
*.color1:       #c4746e
*.color9:       #e46876

! green
*.color2:       #8a9a7b
*.color10:      #87a987

! yellow
*.color3:       #c4b28a
*.color11:      #e6c384

! blue
*.color4:       #8ba4b0
*.color12:      #7fb4ca

! magenta
*.color5:       #a292a3
*.color13:      #938aa9

! cyan
*.color6:       #8ea4a2
*.color14:      #7aa89f

! white
*.color7:       #c8c093
*.color15:      #c5c9c5

XTerm*faceName:         FiraCode Nerd Font
XTerm*faceSize:         11
XTerm*antialias:        true
XTerm*renderFont:       true
XTerm*boldMode:         false
XTerm*fullscreen:       never
XTerm*omitTranslation:  fullscreen

UXTerm*faceName:    FiraCode Nerd Font
UXTerm*faceSize:    12
UXTerm*antialias:   true
UXTerm*renderFont:  true
UXTerm*boldMode:    false

Xcursor*theme: Adwaita
